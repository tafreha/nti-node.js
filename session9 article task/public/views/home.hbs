{{>header}}
{{>navbar}}
<table class="table table-border">
<thead>
	<tr>
		<th>#</th>
		<th> article address </th>
		<th> article content</th>
		<th>action</th>
	</tr>
</thead>
<tbody>
	{{#each articles}}
	<Tr>
		<td>this._id</td>
	<td>{{this.article-address}}</td>
	<td>{{this.article-content}}</td>
	<td>
		<a href="/single/{{this._id}}"
		class="btn btn-primary mx-2">show</a>
		<a href="/edit/{{}}this._id}" class="btn btn-warning">edit</a>
		<a href="/delete/{{this._id}}" class="btn btn-danger">delete</a>
	</td>
	</Tr>
	{{/each}}
</tbody>
</table>

{{>footer}}